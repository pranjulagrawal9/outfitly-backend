{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "brand": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "decimal",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "bestseller": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "trending": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "maincategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::maincategory.maincategory",
      "inversedBy": "products"
    },
    "recommendedScore": {
      "type": "float"
    },
    "popularityScore": {
      "type": "float"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "details": {
      "type": "json",
      "required": true
    },
    "availableSizes": {
      "type": "customField",
      "options": [
        "S",
        "M",
        "L",
        "XL",
        "2XL",
        "3XL"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
